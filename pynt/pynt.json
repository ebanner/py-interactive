{
  "tests": {
    "runner": "nosetests",
    "directory": "codebook/tests"
  },
  "lazy-commands": {
    "codebook/syntax.promote_loop": "ein:connect-run-or-eval-buffer"
  },
  "testable": [
    "codebook/syntax.annotate",
    "codebook/node_transformers.FirstPassForSimple.visit_For"
